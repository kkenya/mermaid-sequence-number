{
  "name": "mermaid-sequence-number",
  "version": "0.0.2",
  "publisher": "kkenya",
  "engines": {
    "vscode": "^1.66.0"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "displayName": "Mermaid sequence number",
  "description": "decorate sequential number for mermaid editor",
  "categories": [
    "Other"
  ],
  "keywords": [
    "mermaid"
  ],
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },
  "preview": true,
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "mermaid",
        "extensions": [
          ".markdown"
        ]
      }
    ]
  },
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "badges": [],
  "markdown": "github",
  "qna": "marketplace",
  "dependencies": {
    "mdast-util-from-markdown": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "ts-loader": "^9.2.8",
    "typescript": "^4.5.5",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "extensionPack": [],
  "extensionDependencies": [],
  "extensionKind": [],
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "icon": "images/icon.png",
  "bugs": {
    "url": "https://github.com/kkenya/mermaid-sequence-number/issues",
    "email": "3982ne@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kkenya/mermaid-sequence-number.git"
  },
  "homepage": "https://github.com/kkenya/mermaid-sequence-number/blob/main/README.md"
}
