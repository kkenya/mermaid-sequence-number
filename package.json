{
  "name": "mermaid-sequence-number",
  "version": "1.2.1",
  "publisher": "kkenya",
  "engines": {
    "vscode": "^1.66.0"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "prepublish": "vsce package",
    "publish": "vsce publish",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "14.x",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.19.0",
    "ts-loader": "^9.2.8",
    "typescript": "^4.5.5",
    "vsce": "^2.7.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "mdast-util-from-markdown": "^1.2.0"
  },
  "displayName": "Mermaid Sequence Number",
  "description": "decorate editor for mermaid sequence diagram",
  "categories": [
    "Other"
  ],
  "keywords": [
    "mermaid"
  ],
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "preview": false,
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "mermaid",
        "extensions": [
          ".markdown",
          ".mmd"
        ]
      }
    ],
    "configuration": {
      "title": "mermaidSequenceNumber",
      "properties": {
        "mermaidSequenceNumber.maxLines": {
          "type": "number",
          "default": 1000,
          "description": "The maximum number of lines",
          "order": 0
        },
        "mermaidSequenceNumber.decorationPosition": {
          "type": "string",
          "default": "after",
          "enum": [
            "before",
            "after"
          ],
          "description": "Decorations are placed either before or after the message",
          "order": 10
        }
      }
    }
  },
  "activationEvents": [
    "onLanguage:markdown",
    "onLanguage:mermaid"
  ],
  "badges": [],
  "markdown": "github",
  "qna": "marketplace",
  "extensionPack": [],
  "extensionDependencies": [],
  "extensionKind": [],
  "icon": "images/icon.png",
  "bugs": {
    "url": "https://github.com/kkenya/mermaid-sequence-number/issues",
    "email": "3982ne@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kkenya/mermaid-sequence-number.git"
  },
  "homepage": "https://github.com/kkenya/mermaid-sequence-number/blob/main/README.md"
}
